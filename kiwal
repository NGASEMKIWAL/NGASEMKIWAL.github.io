    <div class="container">
        <h2>Generator</h2>
        
        <div class="input-group">
            <label for="pattern">Masukkan Angka (0-9 tanpa spasi):</label>
            <input type="text" id="pattern" placeholder="Masukkan angka" oninput="this.value = this.value.replace(/[^0-9]/g, '')">
        </div>
        
        <div class="btn-group">
            <button class="btn" onclick="generatePattern()">Hasilkan</button>
            <button class="btn btn-secondary" onclick="clearAll()">Bersihkan</button>
        </div>
        
        <h3>Kombinasi yang Dihasilkan</h3>
        <textarea id="hasil" readonly></textarea>
        <button class="btn" onclick="copyToClipboard('hasil')">Salin</button>
        
        <h3>Pembalik</h3>
        <textarea id="hasil-missing" readonly></textarea>
        <button class="btn" onclick="copyToClipboard('hasil-missing')">Salin</button>
